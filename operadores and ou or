import os 
os.system("cls")

nota = 10

# Se nota maior ou igual a zero e
# nota menor ou igual a 10
# and -> entre
if nota >= 0 and nota <= 10:
    print(f"Nota: {nota}")
else:
    print("Nota invalida")


nota = 10

# se nota menor que ou maior que 10.
# OR -> Fora do intervalo definido
if nota < 0 or nota > 10:
    print("Nota invalida")
else:
    print(f"Nota: {nota}")
